pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          script {
               currentBuild.displayName = sh (script: 'git log -1 --pretty=%B ${GIT_COMMIT}', returnStdout: true).trim()
                        }        
                         echo "Hello World"
                         sh 'export GOOGLE_APPLICATION_CREDENTIALS="/home/averboonen/alex-verboonen-2022-run-5-927bac94f1b2.json" '
                     }
                }
                 stage("TERRA") {
                     steps {
                          sh 'google-cloud-sdk/bin/gcloud auth activate-service-account --key-file="/home/averboonen/alex-verboonen-2022-run-5-927bac94f1b2.json"'
                          sh 'terraform init'
                          sh 'terrafor plan'
                     }
                }
           }
      }
