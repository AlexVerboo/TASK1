pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          currentBuild.displayName = "$GIT_COMMIT"
                          echo "Estoy cambiando el nombre"
                     }
                }
                 stage("TERRA") {
                     steps {
                          sh 'terraform init'
                          sh 'terrafor plan'
                     }
                }
           }
      }
