pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          def scmVars = checkout([...])
                          echo 'scm : the commit id is ' + scmVars.GIT_COMMIT
                          echo "Estoy cambiando el nombre"
                     }
                }
                 stage("TERRA") {
                     steps {
                          sh 'terraform init'
                          sh 'terrafor plan'
                     }
                }
           }
      }
