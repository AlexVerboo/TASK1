pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          script {
               currentBuild.displayName = sh (script: 'git log -1 --pretty=%B ${GIT_COMMIT}', returnStdout: true).trim()
               export GOOGLE_APPLICATION_CREDENTIALS="/home/user/Downloads/service-account-file.json" 
                        }        
                         echo "Hello World"
                     }
                }
                 stage("TERRA") {
                     steps {
                          sh 'terraform init'
                          sh 'terrafor plan'
                     }
                }
           }
      }
