pipeline {
           agent any
           stages {
                stage("Hello") {
                     steps {
                          shortCommit = sh(returnStdout: true, script: "git log -n 1 --pretty=format:'%h'").trim()
                          echo 'scm : the commit id is ' + shortCommit
                          echo "Estoy cambiando el nombre"
                     }
                }
                 stage("TERRA") {
                     steps {
                          sh 'terraform init'
                          sh 'terrafor plan'
                     }
                }
           }
      }
